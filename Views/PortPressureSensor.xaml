<local:View x:Class="AeonHacs.Wpf.Views.PortPressureSensor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:AeonHacs.Wpf.Views"
             mc:Ignorable="d" >
    <StackPanel>
        <Rectangle Width="9" Height="5"
            Fill="#2A2A2A" Stroke="{DynamicResource EdgeBrush}" StrokeThickness="1" />
        <Rectangle Width="11" Height="4" Margin="0,-1,0,0"
            Fill="WhiteSmoke" Stroke="{DynamicResource EdgeBrush}" StrokeThickness="1" />
        <Rectangle Width="7" Height="5" Margin="0,-1,0,0"
            Fill="#2A2A2A" Stroke="{DynamicResource EdgeBrush}" StrokeThickness="1" />
        <Rectangle Width="5" Height="5" Margin="0,-1,0,0"
            Fill="Gold" Stroke="{DynamicResource EdgeBrush}" StrokeThickness="1" />
    </StackPanel>
</local:View>
